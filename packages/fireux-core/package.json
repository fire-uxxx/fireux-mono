{
  "name": "fireux-core",
  "version": "1.0.0",
  "private": true,
  "main": "./src/module.ts",
  "types": "./src/module.ts",
  "exports": {
    ".": {
      "import": "./src/module.ts",
      "types": "./src/module.ts"
    },
    "./components/*": "./src/runtime/components/*",
    "./runtime/*": "./src/runtime/*",
    "./assets/css/main.css": "./src/runtime/assets/css/main.css",
    "./assets/design-system/main.scss": "./src/runtime/assets/design-system/main.scss",
    "./composables/*": "./src/runtime/composables/**/*",
    "./models/*": "./src/runtime/models/*",
    "./server/*": "./src/runtime/server/**/*",
    "./public/*": "./src/runtime/public/*",
    "./layouts/*": "./src/runtime/layouts/*"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "postbuild": "rimraf src/**/*.map",
    "start": "nuxt start",
    "create-layouts": "node scripts/create-layouts.js"
  },
  "dependencies": {
    "@nuxt/ui": "3.1.3",
    "@vue/compiler-sfc": "^3.5.16",
    "dompurify": "^3.2.6",
    "firebase-admin": "^13.4.0",
    "nuxt": "^3.17.4",
    "nuxt-vuefire": "^1.0.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/kit": "^3.17.5",
    "@nuxt/types": "^2.18.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "cpx": "^1.5.0",
    "glob": "^11.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^3.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.89.1",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "type": "module"
}
