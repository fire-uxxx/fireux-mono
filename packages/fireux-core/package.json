{
  "name": "fireux-core",
  "version": "1.0.0",
  "private": true,
  "main": "./src/module.ts",
  "types": "./src/module.ts",
  "exports": {
    ".": {
      "import": "./src/module.ts",
      "types": "./src/module.ts"
    },
    "./components/*": "./src/runtime/components/*",
    "./runtime/*": "./src/runtime/*",
    "./assets/*": "./src/runtime/assets/*",
    "./composables/*": "./src/runtime/composables/**/*",
    "./models/*": "./src/runtime/models/*",
    "./server/*": "./src/runtime/server/**/*",
    "./public/*": "./src/runtime/public/*"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "dev": "nuxt dev",
    "build": "sass src/runtime/assets/design-system:src/runtime/assets/design-system --no-source-map",
    "postbuild": "rimraf src/**/*.map",
    "compile:scss": "sass src/runtime/assets/design-system:src/runtime/assets/design-system --no-source-map",
    "start": "nuxt start"
  },
  "dependencies": {
    "@nuxt/ui": "3.1.3",
    "dompurify": "^3.2.6",
    "nuxt": "^3.17.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/kit": "^3.17.5",
    "@nuxt/types": "^2.18.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "cpx": "^1.5.0",
    "glob": "^11.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^3.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.89.1",
    "vite-plugin-static-copy": "^3.0.0"
  }
}
