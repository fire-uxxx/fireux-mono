{
  "name": "fireux-places",
  "version": "0.1.0",
  "description": "Location and places management system for FireUX apps with Google Places API integration",
  "private": true,
  "main": "./src/module.ts",
  "types": "./src/module.ts",
  "exports": {
    ".": "./src/module.ts",
    "./composables": "./src/runtime/composables/*"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxt-module-build --stub",
    "postbuild": "rimraf src/**/*.map",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.4",
    "fireux-core": "workspace:*",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.5",
    "rimraf": "^6.0.1"
  },
  "type": "module"
}
