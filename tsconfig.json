{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["packages/fireux-core/src/*"],
      "~models/*": ["packages/fireux-core/src/runtime/models/*"]
    },
    "types": ["@nuxt/schema", "nuxt"]
  },
  "include": ["packages/**/*", "projects/**/*"],
  "exclude": ["node_modules", "dist"]
}
