{
  "name": "fireux",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "// === DEVELOPMENT SERVERS ===": "",
    "dev:all": "concurrently \"pnpm dev:fireux\" \"pnpm dev:misebox\" \"pnpm dev:cleanbox\"",
    "// === FIREUX SYSTEM ===": "",
    "dev:fireux": "pnpm --filter ./projects/fireux/fireux-app dev",
    "// === MISEBOX SYSTEM ===": "",
    "dev:misebox": "pnpm --filter ./projects/misebox/misebox-app dev",
    "dev:niederhorn": "pnpm --filter ./projects/misebox/niederhorn dev",
    "// === CLEANBOX SYSTEM ===": "",
    "dev:cleanbox": "pnpm --filter ./projects/cleanbox/cleanbox-app dev",
    "dev:richies": "pnpm --filter ./projects/cleanbox/richies-reinigung dev",
    "// === PACKAGE MANAGEMENT ===": "",
    "build:packages": "pnpm --filter './packages/*' build",
    "dev:packages": "pnpm --filter './packages/*' dev",
    "clean:packages": "pnpm --filter './packages/*' clean",
    "// === TENANT MANAGEMENT ===": "",
    "create:tenant": "node scripts/create-tenant.js",
    "clean:apps": "rm -rf projects/*/",
    "// === DEVELOPMENT TOOLS ===": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.ts,.vue",
    "format": "prettier --write .",
    "// === STRUCTURE MANAGEMENT ===": "",
    "structure:validate": "./scripts/validate-structure.sh",
    "structure:fix": "./scripts/fix-structure.sh",
    "structure:report": "./scripts/validate-structure.sh && echo 'Report generated'",
    "// === MAINTENANCE ===": "",
    "cleanup:phantom": "./scripts/cleanup-phantom-files.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "concurrently": "^9.2.0",
    "cpx": "^1.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "nuxt": "^3.17.5",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.1",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "dependencies": {
    "better-sqlite3": "^11.10.0"
  },
  "workspaces": [
    "projects/*",
    "packages/*"
  ]
}
