{
  "name": "fireux",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "// === DEVELOPMENT SERVERS ===": "",
    "dev:all": "concurrently \"pnpm dev:fireux\" \"pnpm dev:misebox\" \"pnpm dev:cleanbox\"",
    "dev:all-extended": "concurrently \"pnpm dev:fireux\" \"pnpm dev:misebox\" \"pnpm dev:cleanbox\" \"pnpm dev:niederhorn\" \"pnpm dev:richies\"",
    "// === FIREUX SYSTEM (3000) ===": "",
    "dev:fireux": "pnpm --filter ./projects/fireux/fireux-app dev",
    "// === MISEBOX SYSTEM (3001) ===": "",
    "dev:misebox": "pnpm --filter ./projects/misebox/misebox-app dev",
    "dev:niederhorn": "pnpm --filter ./projects/misebox/niederhorn dev",
    "// === CLEANBOX SYSTEM (3002) ===": "",
    "dev:cleanbox": "pnpm --filter ./projects/cleanbox/cleanbox-app dev",
    "dev:richies": "pnpm --filter ./projects/cleanbox/richies-reinigung dev",
    "// === PACKAGE MANAGEMENT ===": "",
    "build:packages": "pnpm -r --filter './packages/core/*' --filter './packages/extensions/*' --filter './packages/tenants/*' build",
    "dev:packages": "pnpm -r --filter './packages/core/*' --filter './packages/extensions/*' --filter './packages/tenants/*' dev",
    "clean:packages": "pnpm -r --filter './packages/core/*' --filter './packages/extensions/*' --filter './packages/tenants/*' clean",
    "// === NUXT 4 OPTIMIZATIONS ===": "",
    "typecheck": "pnpm -r --parallel --filter './packages/*/*' run build --dry-run || true",
    "prepare:all": "echo 'All apps prepared during postinstall' && pnpm -r --filter './projects/*/*' list --depth=0",
    "build:all": "pnpm -r --parallel --filter './projects/*/*' build",
    "// === TENANT MANAGEMENT ===": "",
    "create:tenant": "node scripts/create-tenant.js",
    "clean:apps": "rm -rf projects/*/",
    "// === DEVELOPMENT TOOLS ===": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.ts,.vue",
    "format": "prettier --write .",
    "// === STRUCTURE MANAGEMENT ===": "",
    "structure:validate": "./scripts/validate-structure.sh",
    "structure:fix": "./scripts/fix-structure.sh",
    "structure:report": "./scripts/validate-structure.sh && echo 'Report generated'",
    "// === MAINTENANCE ===": "",
    "cleanup:phantom": "./scripts/cleanup-phantom-files.sh",
    "// === HEALTH MONITORING ===": "",
    "health": "pnpm why nuxt @nuxt/kit vue vue-router @nuxt/schema && pnpm dedupe --check && pnpm exec tsc -b && pnpm exec vue-tsc --noEmit",
    "health:quick": "pnpm exec tsc -b",
    "health:detailed": "pnpm exec vue-tsc --noEmit",
    "health:nuxt": "pnpm -r --filter \"./projects/**\" nuxi typecheck",
    "health:deps": "pnpm dlx depcheck",
    "health:security": "pnpm audit --prod"
  },
  "volta": {
    "node": "22.9.0",
    "pnpm": "10.12.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.11 <23",
    "pnpm": ">=9"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@tsconfig/nuxt": "^2.0.3",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "concurrently": "^9.2.0",
    "cpx": "^1.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.1",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6"
  },
  "pnpm": {
    "overrides": {
      "nuxt": "4.0.3",
      "@nuxt/kit": "4.0.3",
      "@nuxt/schema": "4.0.3",
      "vue": "3.5.20",
      "vuefire": "3.2.2",
      "firebase": "11.10.0",
      "@iconify-json/lucide": "^1.2.62",
      "@iconify-json/heroicons": "^1.2.2",
      "@iconify-json/logos": "^1.2.5",
      "@iconify-json/skill-icons": "^1.2.0",
      "@iconify-json/vscode-icons": "^1.2.25"
    }
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "dependencies": {
    "better-sqlite3": "^11.10.0"
  },
  "workspaces": [
    "packages/core/*",
    "packages/extensions/*",
    "packages/tenants/*",
    "projects/*/*",
    "playground"
  ]
}
